{
  "name": "generator-koa-bp",
  "version": "0.6.3",
  "description": "An opionated koa api boilerplate",
  "homepage": "https://github.com/drGrove/generator-koa-bp",
  "author": {
    "name": "Danny Grove",
    "email": "danny@drgrovellc.com",
    "url": "https://dannygrove.com"
  },
  "files": [
    "generators/app",
    "generators/route"
  ],
  "keywords": [
    "koa",
    "api",
    "koa api generator",
    "yeoman-generator"
  ],
  "dependencies": {
    "yeoman-generator": "^0.22.0",
    "chalk": "^1.0.0",
    "yosay": "^1.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "eslint": "^3.4.0",
    "eslint-config-google": "^0.5.0",
    "expect": "^1.20.2",
    "gulp": "^3.9.0",
    "gulp-coveralls": "^0.1.0",
    "gulp-eslint": "^3.0.1",
    "gulp-exclude-gitignore": "^1.0.0",
    "gulp-istanbul": "^0.10.3",
    "gulp-line-ending-corrector": "^1.0.1",
    "gulp-mocha": "^2.2",
    "gulp-nsp": "^2.1.0",
    "gulp-plumber": "^1.0.0",
    "sequelize": "^3.24.0",
    "wrench": "^1.5.9",
    "yeoman-assert": "^2.0.0",
    "yeoman-test": "^1.0.0"
  },
  "eslintConfig":{
    "extends": "google",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "parser": "babel-eslint",
    "env": {
      "mocha": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "strict": 0,
      "object-curly-spacing": "off",
      "comma-style": "off",
      "camelcase": "off",
      "generator-star-spacing": "off",
      "array-bracket-spacing": "off",
      "no-spaced-func": "off",
      "space-in-parens": "off",
      "no-unexpected-multiline": "off",
      "max-nested-callbacks": "off"
    }
  },
  "repository": "git@github.com:drGrove/generator-koa-bp",
  "scripts": {
    "prepublish": "gulp prepublish",
    "test": "gulp"
  },
  "license": "MIT"
}
